<?xml version="1.0"?>
<gconf>
	<dir name="schemas">
		<dir name="apps">
			<dir name="docky-2">
				<dir name="WorkspaceSwitcher">
					<dir name="WorkspaceSwitcherDockItem">
						<entry name="WrappedScrolling">
							<local_schema short_desc="Пры згортванні">
								<longdesc>Дазволіць прагортку праз усе стальніцы ў адным кірунку.</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="WeatherDocklet">
					<dir name="WeatherPreferences">
						<entry name="Timeout">
							<local_schema short_desc="Як часта правяраць">
								<longdesc>Як часта правяраць прагноз надвор&apos;я.</longdesc>
							</local_schema>
						</entry>
						<entry name="Source">
							<local_schema short_desc="Крыніца прагнозу надвор&apos;я.">
								<longdesc>Назва крыніцы прагнозу надвор&apos;я для атрымання звестак аб надвор&apos;і.</longdesc>
							</local_schema>
						</entry>
						<entry name="Metric">
							<local_schema short_desc="Выкарыстоўваць метрычную сістэму">
								<longdesc>Калі так, прагноз надвор&apos;я будзе ў метрычных адзінках.</longdesc>
							</local_schema>
						</entry>
						<entry name="Location">
							<local_schema short_desc="Адрасы для прагнозу надвор&apos;я">
								<longdesc>Спіс адрэсаў для праверкі прагнозу надвор&apos;я.</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="Timer">
					<dir name="TimerMainDockItem">
						<entry name="DismissOnClick">
							<local_schema short_desc="Скінуць спынены таймер па пстрычцы мышы.">
								<longdesc>Калі не, завершаныя таймеры будуць перазапускацца, а не абнуляецца.</longdesc>
							</local_schema>
						</entry>
						<entry name="DefaultTimer">
							<local_schema short_desc="Тэрмін дзеяння новага таймера">
								<longdesc>Тыповы час (у сек.) Для новых таймераў.</longdesc>
							</local_schema>
						</entry>
						<entry name="AutoStartTimers">
							<local_schema short_desc="Запусціць ўсе новыя таймеры">
								<longdesc>Калі так, усе новыя таймеры будуць запускацца аўтаматычна.</longdesc>
							</local_schema>
						</entry>
						<entry name="AutoDismissTimers">
							<local_schema short_desc="Скінуць завершаныя таймеры">
								<longdesc>Калі так, таймеры будзе знікаць, калі час выйшаў.</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="SessionManager">
					<dir name="SessionManagerItem">
						<entry name="CurrentIndex">
							<local_schema short_desc="Абраны элемент">
								<longdesc>Абраны элемент меню мэнэджара сеансу</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="RecentDocuments">
					<dir name="RecentDocumentsItem">
						<entry name="NumRecentDocs">
							<local_schema short_desc="Колькасць нядаўніх дакументаў">
								<longdesc>Колькасць нядаўніх дакументаў да паказу.</longdesc>
							</local_schema>
						</entry>
						<entry name="AlwaysShowRecent">
							<local_schema short_desc="Паказваць найноўшыя дакументы">
								<longdesc>Калі так, на панэлі будуць паказвацца нядаўнія дакументы.</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="NPR">
					<dir name="NPR">
						<entry name="MyStations">
							<local_schema short_desc="NPR-радыёстанцыі">
								<longdesc>Спіс NPR-станцый да паказу.</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="GMail">
					<dir name="GMailPreferences">
						<entry name="User">
							<local_schema short_desc="Імя карыстальніка GMail">
								<longdesc>Імя карыстача GMail для праверкі пошты.</longdesc>
							</local_schema>
						</entry>
						<entry name="RefreshRate">
							<local_schema short_desc="Як часта правяраць">
								<longdesc>Як часта правяраць пошту (у хвілінах).</longdesc>
							</local_schema>
						</entry>
						<entry name="Notify">
							<local_schema short_desc="Паказваць абвесткі пра не прачытаныя лісты">
								<longdesc>Калі так, будуць паказвацца абвесткі пра не прачытаныя лісты.</longdesc>
							</local_schema>
						</entry>
						<entry name="NeedsAttention">
							<local_schema short_desc="Паказваць кропку, калі ёсць непрачытаныя лісты">
								<longdesc>Калі так, Docky будзе паказваць індыкатарную кропку, калі ё прачытаныя лісты.</longdesc>
							</local_schema>
						</entry>
						<entry name="LastChecked">
							<local_schema short_desc="Час апошняй праверкі">
								<longdesc>Час апошняй праверкі GMail на новыя лісты.</longdesc>
							</local_schema>
						</entry>
						<entry name="Labels">
							<local_schema short_desc="Адзнакі для праверкі GMail">
								<longdesc>Спіс нетыповых метак GMail для праверкі на даданне ва «ўваходныя».</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="Docky">
					<dir name="Services">
						<dir name="ThemeService">
							<entry name="UrgentHue">
								<local_schema short_desc="Змены колеру індыкатара &quot;тэрмінова&quot;">
									<longdesc>На колькі змяніць колер індыкатара &quot;тэрмінова&quot;. Значэнне можа быць ад -180 да 180.</longdesc>
								</local_schema>
							</entry>
							<entry name="Theme">
								<local_schema short_desc="Бягучая тэма">
									<longdesc>Назва бягучай тэмы Docky.</longdesc>
								</local_schema>
							</entry>
							<entry name="GlowTime">
								<local_schema short_desc="Час ззяння індыкатара &quot;тэрмінова&quot; (у секундах)">
									<longdesc>Час анімацыі ззяння тэрміновага індыкатара (у секундах), 0 - выключаны, -1 - неабмежавана.</longdesc>
								</local_schema>
							</entry>
						</dir>
						<dir name="HelperService">
							<entry name="ShowOutput">
								<local_schema short_desc="Перанакіраваць вывад дадаткаў да высновы Docky">
									<longdesc>Калі так, вывад прыкладанняў перанакіроўваецца на вывад Docky.</longdesc>
								</local_schema>
							</entry>
						</dir>
					</dir>
					<dir name="Items">
						<dir name="WnckDockItem">
							<entry name="ShowHovers">
								<local_schema short_desc="Паказваць падказкі для кнопак запуску">
									<longdesc>Калі не, Docky не будзе паказваць падказкі для значак пад паказальнікам (але паказваць для модуляў).</longdesc>
								</local_schema>
							</entry>
							<entry name="CurrentDesktopOnly">
								<local_schema short_desc="Паказваць вокны толькі з бягучай стальніцы">
									<longdesc>Калі так, Docky будзе паказваць вокны толькі на актыўнай стальніцы.</longdesc>
								</local_schema>
							</entry>
						</dir>
						<dir name="FileApplicationProvider">
							<entry name="AllowPinToDock">
								<local_schema short_desc="Паказваць пункт «Замацаваць на панэлі»">
									<longdesc>Калі параметр «Замацаваць на панэлі» будзе ў меню элементаў, дазволіць дадаваць элементы ў панэлі.</longdesc>
								</local_schema>
							</entry>
						</dir>
						<dir name="DockyItem">
							<entry name="ShowSettings">
								<local_schema short_desc="Паказваць пункт меню Docky «Наладкі»">
									<longdesc>Калі так, кантэкстнае меню Docky будзе ўтрымліваць пункт «Параметры».</longdesc>
								</local_schema>
							</entry>
							<entry name="ShowQuit">
								<local_schema short_desc="Паказваць пункт меню Docky на &quot;Выхад&quot;">
									<longdesc>Калі так, кантэкстнае меню Docky будзе ўтрымліваць пункт &quot;Выхад&quot;.</longdesc>
								</local_schema>
							</entry>
							<entry name="ShowDockyItem">
								<local_schema short_desc="Паказваць значку «Docky»">
									<longdesc>Калі так, будзе паказаны значок Docky на панэлі кіравання вокнамі.</longdesc>
								</local_schema>
							</entry>
							<entry name="Hue">
								<local_schema short_desc="Адценне элемента Docky">
									<longdesc>Колер значка Docky. Значэнне 0 - выкарыстоўваць колер тэмы GTK +.</longdesc>
								</local_schema>
							</entry>
							<entry name="HoverText">
								<local_schema short_desc="Тэкст падказкі элемента Docky">
									<longdesc>Тэкст для паказу пад паказальнікам на элеменце ў Docky.</longdesc>
								</local_schema>
							</entry>
							<entry name="DockyItemCommand">
								<local_schema short_desc="Выкананая каманда элемента Docky">
									<longdesc>Каманда, якая будзе выконвацца пры пстрычцы левай кнопкай мышы на элеменце Docky, або &quot;&quot; для тыповых паводзінаў (гэта значыць, адкрыць дыялог налады).</longdesc>
								</local_schema>
							</entry>
						</dir>
					</dir>
					<dir name="Interface">
						<dir name="DockPreferences">
							<entry name="FirstRun">
								<local_schema short_desc="Не змяняйце гэта!">
									<longdesc>Калі так, гэта першы запуск Docky. Толькі для ўнутранага карыстання: Не змяняйце гэтае значэнне!</longdesc>
								</local_schema>
							</entry>
							<dir name="DockN">
								<entry name="ZoomPercent">
									<local_schema short_desc="Памер павелічэння">
										<longdesc>Маштабаванне панэлі (калі уключаны), ў адсотках.</longdesc>
									</local_schema>
								</entry>
								<entry name="ZoomEnabled">
									<local_schema short_desc="Маштабаванне панэлі">
										<longdesc>Калі так, панэль будзе пашырацца пад паказальнікам.</longdesc>
									</local_schema>
								</entry>
								<entry name="WindowManager">
									<local_schema short_desc="Панэль кіруе ўсімі вокнамі">
										<longdesc>Калі так, усе вокны без значак будуць паказвацца на гэтай панэлі. Па меншай ступені, адна панэль мусіць мець значэнне «так».</longdesc>
									</local_schema>
								</entry>
								<entry name="ThreeDimensional">
									<local_schema short_desc="Панэль у 3D">
										<longdesc>Калі так, панэль будзе ў рэжыме 3D.</longdesc>
									</local_schema>
								</entry>
								<entry name="SortList">
									<local_schema short_desc="Сартуе элементы на панэлі">
										<longdesc>Выкарыстоўваецца для сартавання элементаў на панэлі.</longdesc>
									</local_schema>
								</entry>
								<entry name="Position">
									<local_schema short_desc="Размяшчэнне панэлі">
										<longdesc>Размяшчэнне панэлі на экране. Магчымыя значэнні: «Унізе», «Наверсе», «Налева» і «Направа»</longdesc>
									</local_schema>
								</entry>
								<entry name="Plugins">
									<local_schema short_desc="Модулі на гэтай панэлі">
										<longdesc>Спіс надбудоў, якія выкарыстоўваюцца на гэтай панэлі</longdesc>
									</local_schema>
								</entry>
								<entry name="PanelMode">
									<local_schema short_desc="Калі док з&apos;яўляецца панэллю">
										<longdesc>Калі так, панэль будзе пашырацца на ўсю шырыню экрана.</longdesc>
									</local_schema>
								</entry>
								<entry name="MonitorNumber">
									<local_schema short_desc="Манітор, на якім размешчаная панэль">
										<longdesc>Манітор, на якім размешчаная панэль.</longdesc>
									</local_schema>
								</entry>
								<entry name="Launchers">
									<local_schema short_desc="Спіс кнопак запуску">
										<longdesc>Спіс усіх значак запуску на гэтай панэлі.</longdesc>
									</local_schema>
								</entry>
								<entry name="IndicateMultipleWindows">
									<local_schema short_desc="Паказваць, калі элемент мае некалькі вокнаў">
										<longdesc>Калі так, элементы, якія маюць больш аднаго акна, будуць пазначацца двума кропкамі замест адной.</longdesc>
									</local_schema>
								</entry>
								<entry name="IconSize">
									<local_schema short_desc="Памер значкак панэлі">
										<longdesc>Памер значак на панэлі.</longdesc>
									</local_schema>
								</entry>
								<entry name="HotAreaPadding">
									<local_schema short_desc="Актыўная вобласць запаўнення">
										<longdesc>Колькасць дадатковых пікселяў для дадатку вобласці навядзення ў пачатку панэлі.</longdesc>
									</local_schema>
								</entry>
								<entry name="FadeOpacity">
									<local_schema short_desc="Празрыстасць, да якой «знікаць»">
										<longdesc>Празрыстасць панэлі падчас паступовага знікнення. 1 - суцэльная, 2 - нябачная.</longdesc>
									</local_schema>
								</entry>
								<entry name="FadeOnHide">
									<local_schema short_desc="Зацяненне падчас ўтойвання">
										<longdesc>Калі так, панэль паступова будзе знікаць замест слізгацення за край экрана.</longdesc>
									</local_schema>
								</entry>
								<entry name="Autohide">
									<local_schema short_desc="Рэжым ўтойвання панэлі">
										<longdesc>Рэжым ўтойвання панэлі. Магчымыя значэння: «Нічога», «Аўта-ўтойванне», «Разумнае ўтойванне» і «Пазбягаць вокнаў»</longdesc>
									</local_schema>
								</entry>
							</dir>
						</dir>
						<dir name="DockDragTracker">
							<entry name="ProvidersAcceptDrops">
								<local_schema short_desc="Забараніць даданне элементаў перацягваннем">
									<longdesc>Калі не, блакуецца даданне элементаў да панэлі перацягваннем.</longdesc>
								</local_schema>
							</entry>
							<entry name="LockDrags">
								<local_schema short_desc="Прыпыніць перацягванне элементаў">
									<longdesc>Калі так, усе элементы на ўсіх панэлях блакуюцца ад перацягвання.</longdesc>
								</local_schema>
							</entry>
						</dir>
						<dir name="AutohideManager">
							<entry name="UpdateDelay">
								<local_schema short_desc="Час чакання абнаўлення (у мс) пасля змены акна">
									<longdesc>Час чакання пераключэння акна, які прыводзіць да з&apos;яўлення або знікнення панэлі (у мілісекундах).</longdesc>
								</local_schema>
							</entry>
							<entry name="UnhideDelay">
								<local_schema short_desc="Затрымка перад паказам панэлі (у мс)">
									<longdesc>Час чакання з&apos;яўлення схаванай панэлі пад паказальнікам мышы (у мілісекундах).</longdesc>
								</local_schema>
							</entry>
						</dir>
					</dir>
					<dir name="DockController">
						<entry name="CompositeCheckEnabled">
							<local_schema short_desc="Ўключэнне праверкі на сумяшчальнасць">
								<longdesc>Калі пры праверцы сумяшчальнасці немагчымая, Docky, паведаміць пра гэта.</longdesc>
							</local_schema>
						</entry>
						<entry name="ActiveDocks">
							<local_schema short_desc="Спіс актыўных докаў">
								<longdesc>Спіс усіх актыўных на дадзены момант (паказаных) панэляў, па назве.</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="CPUMonitor">
					<dir name="CPUMonitorDockItem">
						<entry name="ClickCommand">
							<local_schema short_desc="Каманда пры пстрычцы левай кнопкай">
								<longdesc>Гэтая каманда будзе выконвацца пры пстрычцы левай кнопкай мышы на CPU-доклеце.</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="Clock">
					<dir name="ClockDockItem">
						<entry name="ShowMilitary">
							<local_schema short_desc="24-гадзінны час">
								<longdesc>Калі так, час будзе паказвацца ў 24-гадзіннікавым фармаце.</longdesc>
							</local_schema>
						</entry>
						<entry name="ShowDigital">
							<local_schema short_desc="Паказваць лічбавы гадзіннік">
								<longdesc>Калі так, будзе паказвацца лічбавы гадзіннік замест аналагавага.</longdesc>
							</local_schema>
						</entry>
						<entry name="ShowDate">
							<local_schema short_desc="Паказваць дату">
								<longdesc>Калі так, лічбавы гадзіннік будзе паказваць дату.</longdesc>
							</local_schema>
						</entry>
						<entry name="ClockTheme">
							<local_schema short_desc="Тэма з аналагавым гадзіннікам">
								<longdesc>Тэма аналагавага гадзінніка.</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="Clippy">
					<dir name="ClippyItem">
						<entry name="TrackMouseSelections">
							<local_schema short_desc="Адсочваць выбар мышшу">
								<longdesc>Калі так, у буферы будуць адсочвацца варыянты выбару мышшу. Калі няма, будзе адсочвацца другасны буфер.</longdesc>
							</local_schema>
						</entry>
						<entry name="TimerDelay">
							<local_schema short_desc="Затрымка таймера">
								<longdesc>Як часта правяраць буфер на новыя дадзеныя (у мін.).</longdesc>
							</local_schema>
						</entry>
						<entry name="MaxEntries">
							<local_schema short_desc="Макс. колькасць запісаў">
								<longdesc>Колькі паказваць нядаўніх элементаў выбару ў буферы.</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
			</dir>
		</dir>
	</dir>
</gconf>
