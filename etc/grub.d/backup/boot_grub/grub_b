	menuentry 'Linux Mint 17.2 Cinnamon 64-bit, with Linux 3.16.0-38-generic (on /dev/sda9)' --class gnu-linux --class gnu --class os $menuentry_id_option 'osprober-gnulinux-/vmlinuz-3.16.0-38-generic--186fe816-593e-4f7c-ac0c-fb4be7ec86a8' {
