	else
	  search --no-floppy --fs-uuid --set=root 63c7f581-6dcf-4c60-bb89-e46185676607
	fi
	linux /vmlinuz-3.16.0-38-generic root=UUID=186fe816-593e-4f7c-ac0c-fb4be7ec86a8 ro quiet splash $vt_handoff
	initrd /initrd.img-3.16.0-38-generic
}
submenu 'Advanced options for Linux Mint 17.2 Rafaela (17.2) (on /dev/sda9)' $menuentry_id_option 'osprober-gnulinux-advanced-186fe816-593e-4f7c-ac0c-fb4be7ec86a8' {
